{
  "name": "fe-proxy-v3",
  "short_name": "fe-proxy-v3",
  "version": "3.0.0",
  "description": "一个用于重定向 URL 并允许 CORS 的工具，使本地开发体验变得更加简单。",
  "manifest_version": 3,
  "declarative_net_request": {
    "rule_resources": []
  },
  "action": {
    "default_icon": {
      "16": "./icons/icon-16.png",
      "24": "./icons/icon-24.png",
      "32": "./icons/icon-32.png"
    },
    "default_title": "fe-proxy-v3",
    "default_popup": "index.html"
  },
  "permissions": [
    "sidePanel",
    "storage",
    "unlimitedStorage",
    "browsingData",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback",
    "declarativeNetRequestWithHostAccess"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "48": "./icons/icon-32.png",
    "128": "./icons/icon-32.png"
  },
  "side_panel": {
    "default_path": "index.html"
  },
  "background": {
    "service_worker": "service_worker.min.js"
  }
}
