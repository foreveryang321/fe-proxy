{
  "name": "fe-proxy",
  "short_name": "fe-proxy",
  "version": "3.0.0",
  "description": "一个用于重定向 URL 并允许 CORS 的工具，使本地开发体验变得更加简单。",
  "manifest_version": 3,
  "declarative_net_request": {
    "rule_resources": []
  },
  "permissions": [
    "sidePanel",
    "storage",
    "unlimitedStorage",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback",
    "declarativeNetRequestWithHostAccess"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "action": {
    "default_icon": {
      "16": "./icons/icon-16.png",
      "24": "./icons/icon-24.png",
      "32": "./icons/icon-32.png"
    },
    "default_title": "fe-proxy"
  },
  "side_panel": {
    "default_path": "dist/index.html"
  },
  "icons": {
    "16": "./icons/icon-16.png",
    "24": "./icons/icon-24.png",
    "32": "./icons/icon-32.png",
    "48": "./icons/icon-32.png",
    "128": "./icons/icon-32.png"
  },
  "background": {
    "service_worker": "service_worker.min.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./content.min.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "index.html"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
